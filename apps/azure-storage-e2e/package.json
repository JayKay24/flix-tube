{
  "name": "@flix-tube/azure-storage-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "azure-storage-e2e",
    "implicitDependencies": [
      "azure-storage"
    ],
    "targets": {
      "start-dev-env": {
        "executor": "nx:run-commands",
        "options": {
          "command": "source ./dev_utility_functions.sh && up prod"
        }
      },
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/apps/azure-storage-e2e/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/azure-storage-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "start-dev-env"
        ]
      }
    }
  }
}
