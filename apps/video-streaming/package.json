{
  "name": "@flix-tube/video-streaming",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "webpack-cli build",
          "args": [
            "--node-env=production"
          ],
          "cwd": "apps/video-streaming"
        },
        "configurations": {
          "development": {
            "args": [
              "--node-env=development"
            ]
          }
        }
      },
      "prune-lockfile": {
        "dependsOn": [
          "build"
        ],
        "cache": true,
        "executor": "@nx/js:prune-lockfile",
        "outputs": [
          "{workspaceRoot}/apps/video-streaming/dist/package.json",
          "{workspaceRoot}/apps/video-streaming/dist/package-lock.json"
        ],
        "options": {
          "buildTarget": "build"
        }
      },
      "copy-workspace-modules": {
        "dependsOn": [
          "build"
        ],
        "cache": true,
        "outputs": [
          "{workspaceRoot}/apps/video-streaming/dist/workspace_modules"
        ],
        "executor": "@nx/js:copy-workspace-modules",
        "options": {
          "buildTarget": "build"
        }
      },
      "prune": {
        "dependsOn": [
          "prune-lockfile",
          "copy-workspace-modules"
        ],
        "executor": "nx:noop"
      },
      "serve": {
        "continuous": true,
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "@flix-tube/video-streaming:build",
          "runBuildTargetDependencies": false
        },
        "configurations": {
          "development": {
            "buildTarget": "@flix-tube/video-streaming:build:development"
          },
          "production": {
            "buildTarget": "@flix-tube/video-streaming:build:production"
          }
        }
      },
      "test": {
        "options": {
          "passWithNoTests": true,
          "coverage": true,
          "coverageDirectory": "test-coverage",
          "collectCoverageFrom": [
            "**/*.(t|j)s",
            "!main.(t|j)s",
            "!**/*.module.(t|j)s",
            "!**/*.dto.(t|j)s",
            "!**/*.entity.(t|j)s",
            "!**/*.args.(t|j)s",
            "!**/*.factories.(t|j)s",
            "!**/*.types.(t|j)s",
            "!**/*.schema.(t|j)s",
            "!**/node_modules/**"
          ]
        }
      }
    }
  },
  "dependencies": {}
}
